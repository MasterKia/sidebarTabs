{
  "name": "sidebartabs",
  "description": "Emulate tabs in sidebar.",
  "author": "asamuzaK",
  "homepage": "https://github.com/asamuzaK/sidebarTabs",
  "bugs": {
    "url": "https://github.com/asamuzaK/sidebarTabs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asamuzaK/sidebarTabs.git"
  },
  "license": "MPL-2.0",
  "type": "module",
  "scripts": {
    "lint": "npm-run-all -s lint:*",
    "lint:addons-linter": "addons-linter src",
    "lint:eslint": "eslint --fix .",
    "lint:style": "stylelint --fix src/css/*.css",
    "test": "npm run test:central",
    "test:beta": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=beta --exit",
    "test:central": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=central --exit",
    "test:esr": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=esr --exit",
    "test:release": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=release --exit",
    "test_all": "npm-run-all -s test:*"
  },
  "devDependencies": {
    "addons-linter": "^3.19.0",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jsdom": "^18.0.0",
    "mocha": "^9.1.2",
    "npm-run-all": "^4.1.5",
    "psl": "^1.8.0",
    "sinon": "^11.1.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "webext-schema": "^2.0.0"
  },
  "version": "6.2.0"
}
