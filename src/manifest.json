{
  "applications": {
    "gecko": {
      "id": "sidebarTabs@asamuzak.jp",
      "strict_min_version": "59.0a1"
    }
  },
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "img/icon.svg#context",
    "default_title": "__MSG_extensionName__"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "js/twitter.js"
      ],
      "matches": ["https://twitter.com/*"],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/asamuzaK/sidebarTabs",
  "icons": {
    "32": "img/icon.svg#context",
    "64": "img/icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "bookmarks",
    "contextualIdentities",
    "cookies",
    "management",
    "sessions",
    "storage",
    "tabs"
  ],
  "short_name": "__MSG_extensionShortName__",
  "sidebar_action": {
    "browser_style": false,
    "default_icon": "img/icon.svg#context",
    "default_panel": "html/sidebar.html",
    "default_title": "__MSG_extensionShortName__"
  },
  "version": "0.28.0a2"
}
